{
  "name": "marcotoniut.github.io",
  "private": true,
  "scripts": {
    "build": "next build",
    "deploy": "gh-pages -d out -t",
    "dev": "next",
    "dev:pdf": "cross-env PDF_PRINTING=true next -p 3001",
    "lint": "next lint",
    "predeploy": "rm -rf node_modules/.cache && rm -rf out/ && yarn build && touch out/.nojekyll",
    "print-cv": "yarn node scripts/print-cv.js",
    "start": "next start"
  },
  "dependencies": {
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@vanilla-extract/next-plugin": "^2.3.2",
    "child_process": "^1.0.2",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "^14.0.4",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.4",
    "puppeteer": "^21.7.0",
    "typescript": "4.5.2"
  },
  "packageManager": "yarn@4.0.2"
}
